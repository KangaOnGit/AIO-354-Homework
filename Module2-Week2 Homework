        "#Exercise 2\n",
        "\n",
        "bg1_image = cv2.imread ('GreenBackground.png', 1) #Actual Background\n",
        "bg1_image = cv2.resize (bg1_image , (678,381))\n",
        "\n",
        "ob_image = cv2.imread ('Object.png', 1) #Obj with green background\n",
        "ob_image = cv2.resize (ob_image , (678,381))\n",
        "\n",
        "bg2_image = cv2.imread ('NewBackground.jpg', 1) #New Background 2 Input\n",
        "bg2_image = cv2.resize (bg2_image , (678,381))\n",
        "\n",
        "\n",
        "\n",
        "def compute_absolute_difference(bg1_image, ob_image):\n",
        "  diff = cv2.absdiff(bg1_image, ob_image) #Create a new image(kinda) that is the absolute diff of background img and the object img with the bg\n",
        "  difference_binary = cv2.threshold(diff, 15, 255, cv2.THRESH_BINARY) #If the color pixel is > 15, converts it to 255 and if its < 255, converts to 0/Black\n",
        "  return difference_binary\n",
        "\n",
        "difference_binary = compute_absolute_difference(bg1_image, ob_image)\n",
        "New_Image_With_Background = np.where(difference_binary == 0, bg2_image, ob_image) #If it's black then switch it with the background image 2\n"
      ],
