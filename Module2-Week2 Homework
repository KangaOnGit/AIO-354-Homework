      "source": [
        "import numpy as np\n",
        "import math\n",
        "\n",
        "def compute_vector_length (vector):\n",
        "  len_of_vector = (sum([x**2 for x in vector])**(1/2)) # Change the generator to a list comprehension\n",
        "  return len_of_vector\n",
        "\n",
        "a =compute_vector_length([1,2,3])\n",
        "print (a)\n",
        "\n",
        "def compute_dot_prod (vector1, vector2):\n",
        "  result = np.array(vector1).dot(np.array(vector2))\n",
        "  return result\n",
        "\n",
        "def matrix_multi_vector (matrix, vector3):\n",
        "  result = matrix*np.array(vector3)\n",
        "  return result\n",
        "\n",
        "def matrix_multi_matrix (matrix1, matrix2):\n",
        "\n",
        "  matrix2 = np.reshape(matrix2, (np.shape(matrix1)[1], np.shape(matrix2)[0]))\n",
        "\n",
        "  result = np.matmul(matrix1, matrix2)\n",
        "\n",
        "  return result\n",
        "\n",
        "def inverse_matrix (matrix3):\n",
        "  result = np.linalg.inv(matrix3)\n",
        "  return result\n",
        "\n",
        "def compute_eigenvalues_eigenvectors (matrix4):\n",
        "  eigenvalues_eigenvectors = np.linalg.eig(matrix4)\n",
        "  return eigenvalues_eigenvectors\n",
        "\n",
        "#Markdown bổ sung sau, em không hiểu Identity Matrix với cách tìm lamda (Nếu Ax = lambda x => lambda = A (?))\n",
        "\n",
        "def compute_cosine (vector1, vector2):\n",
        "  cosine_similarity = np.dot(vector1,vector2)/(norm(vector1, axis=1)*norm(vector2))\n",
        "  return cosine_similarity\n",
        "\n",
        "test1 = [[-2, 6], [8, -4]]\n",
        "test2 = ([2, 3, 4], [4, 5, 6])\n",
        "print (inverse_matrix(test1))"
